    g.add_state()
    g.add_state()
    g.add_state()
    g.add_state()
    g.add_state()
    g.add_state()
    g.add_state()
    g.add_state()
    g.add_state()
    g.add_state()
    g.add_state()
    g.add_state()
    g.add_state()
    g.add_state()
    g.add_state()
    g.add_state()
    g.add_state()
    g.add_state()
    g.add_state()
    g.add_state()
    g.add_state()
    g.add_state()
    g.add_state()
    g.add_state()
    

