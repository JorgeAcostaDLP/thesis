    return(t)
    
def getmfst(melodytotest,t):
    m = fst.Fst()

    for i in range(len(melodytotest)):
        m.add_state()

    m.set_start(0)

    for i in range(len(melodytotest)-1):
        arc = fst.Arc(melodytotest[i],melodytotest[i],1,i+1)
        m.add_arc(i,arc)

    m.set_final(len(melodytotest)-1)
    
    t.arcsort('ilabel')

    c = fst.compose(m,t)
    #c.arcsort('olabel')
    sequence = fst.shortestpath(c)

    return [m,c,sequence]

def printsequence(sequence):
    chords = []
    notes = []
    for state in sequence.states():
        for arc in sequence.arcs(state):
            notes.append(arc.ilabel)
            chords.append(arc.olabel)
    return
