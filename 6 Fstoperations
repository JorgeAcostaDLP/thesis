import pywrapfst as fst
import numpy as np

def log_plus(x,y):
    """ Perform addition in the log semiring, i.e.
    Parameters
    ----------
    x: float
        weight
    y: float
        weight        
    Returns
    -------
    xplusy: float
        result of log-semiring addition
    """

    xplusy = -np.log(np.exp(-x) + np.exp(-y))
    return xplusy


def numarcsandstates(f):  
    return sum(1 + f.num_arcs(s) for s in f.states())

def fstdata(f):
    for state in f.states():
        for arc in f.arcs(state):
            print state, arc.ilabel, arc.olabel, arc.weight, arc.nextstate
    return

def proposedseq(sequence):
    proposal = []
    for state in sequence.states():
        for arc in sequence.arcs(state):
            proposal.append(arc.olabel)
    return (proposal)



def getLfst(totalPairWeights, totalBigramWeights):
    we = []

    for i in range(len(totalPairWeights)):
#        we.append(-(np.log(totalPairWeights[i]+1)
        
        we.append(totalPairWeights[i])

    for i in range(len(we)):
        if we[1] == 0:
            we[1] == .001
        if we[i] != 0:
            we[i]=1/we[i]
#        
        
#Empty Fst
    f = fst.Fst()
    g = fst.Fst()

#Initial State
    f.add_state()
    f.set_start(0)


    g.add_state()
    g.add_state()
    g.add_state()
    g.add_state()
    g.add_state()
    g.add_state()
    g.add_state()
    g.add_state()
    g.add_state()
    g.add_state()
    g.add_state()
    g.add_state()
